SELECT        dbo.FCD_URUNAGAC.*, dbo.FCD_FIYATLARSATINALMA.AvgPrice2024 AS [2024 ORTALAMA], dbo.FCD_FIYATLARSATINALMA.LastPrice2024 AS [2024 SON FÄ°YAT]
FROM            dbo.FCD_URUNAGAC INNER JOIN
                         dbo.FCD_FIYATLARSATINALMA ON dbo.FCD_URUNAGAC.[HAMMADDE KODU] = dbo.FCD_FIYATLARSATINALMA.CODE